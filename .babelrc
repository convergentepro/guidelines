{
    "presets": [[

         "@babel/preset-env",
         {
          "modules": false,
           "targets": {

             "edge": "17",
             "firefox": "60",
             "chrome": "67",
             "safari": "11.1"
            },
            "useBuiltIns": "usage",
            "corejs": "3.22.7"
          }
        ]
      ],
      
      "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-function-bind",
        "@babel/plugin-syntax-dynamic-import"
    ]
}


