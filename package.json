{
    "name": "convergente-guidelines",
    "version": "1.3.0",
    "description": "Branding Guidelines of my trade mark professional",
    "main": "",
    "repository": "https://github.com/convergentepro/guidelines.github.io.git",
    "author": "Sergio Jurado <sergiojurado.casado@gmail.com>",
    "license": "MIT",
    "private": false,
    "type": "module",
    "scripts": {
        "start": "nodemon --exec babel-node src/index.js",
        "build": "vite build -c vite.config.js",
        "dev": "concurrently --kill-others \"npm run sass\" \"npm run postcss\" \"vite serve -c vite.config.js\"",
        "sass": "sass -w --verbose src/scss/index.scss:src/css/index.css",
        "postcss": "postcss --verbose -w src/css/index.css -o src/css/index.css --use=autoprefixer"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.18.2",
        "@babel/node": "^7.17.10",
        "@babel/plugin-proposal-class-properties": "^7.17.12",
        "@babel/plugin-proposal-function-bind": "^7.16.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.18.2",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.18.2",
        "@babel/runtime": "^7.18.3",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.44",
        "autoprefixer": "^10.4.7",
        "core-js": "^3.22.7",
        "cssnano": "^5.1.9",
        "dotenv": "^16.0.1",
        "postcss": "^8.4.14",
        "postcss-cli": "^9.1.0",
        "postcss-scss": "^4.0.4",
        "sass": "^1.52.1",
        "svelte-preprocess": "^4.10.6",
        "vite": "^2.9.9"
    }
}
