        "mini-css-extract-plugin": "^2.6.0",
        "postcss": "^8.4.14",
        "postcss-cli": "^9.1.0",
        "postcss-scss": "^4.0.4",
        "sass": "^1.52.1",
        "sass-loader": "^13.0.0",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.10.6",
        "terser-webpack-plugin": "^5.3.1",
        "typescript": "^4.7.2",
