    "license": "MIT",
    "private": false,
    "type": "module",
    "scripts": {
        "start": "nodemon --exec babel-node src/index.js",
        "build": "vite build -c vite.config.js",
        "dev": "concurrently --kill-others \"npm run sass\" \"npm run postcss\" \"vite serve -c vite.config.js\"",
        "sass": "sass -w --verbose src/scss/index.scss:src/css/index.css",
        "postcss": "postcss --verbose -w src/css/index.css -o src/css/index.css --use=autoprefixer"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.18.2",
